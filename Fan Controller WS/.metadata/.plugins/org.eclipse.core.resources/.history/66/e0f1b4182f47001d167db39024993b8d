#ifndef DC_MOTOR_H_
#define DC_MOTOR_H_
#include "gpio.h"

#define DC_MOTOR_PIN0_PORT PORTB_ID
#define DC_MOTOR_PIN1_PORT PORTB_ID

#define DC_MOTOR_PIN0 PIN0_ID
#define DC_MOTOR_PIN1 PIN1_ID

typedef enum{
	STOP, CW, ACW
}DcMotor_State;

#endif /* DC_MOTOR_H_ */
